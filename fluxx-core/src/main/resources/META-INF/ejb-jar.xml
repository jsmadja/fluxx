<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee" version="3.1"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd">
	<enterprise-beans>
		<session>
			<ejb-name>AsynchronousFeedDownloaderService</ejb-name>
			<ejb-class>fr.fluxx.core.service.AsynchronousFeedDownloaderService</ejb-class>
			<session-type>Stateless</session-type>
		</session>
		<session>
			<ejb-name>CategoryService</ejb-name>
			<ejb-class>fr.fluxx.core.service.CategoryService</ejb-class>
			<session-type>Stateless</session-type>
		</session>
		<session>
			<ejb-name>CustomFeedService</ejb-name>
			<ejb-class>fr.fluxx.core.service.CustomFeedService</ejb-class>
			<session-type>Stateless</session-type>
		</session>
		<session>
			<ejb-name>DownloadableItemService</ejb-name>
			<ejb-class>fr.fluxx.core.service.DownloadableItemService</ejb-class>
			<session-type>Stateless</session-type>
		</session>
		<session>
			<ejb-name>FeedFetcherService</ejb-name>
			<ejb-class>fr.fluxx.core.service.FeedFetcherService</ejb-class>
			<session-type>Stateless</session-type>
		</session>
		<session>
			<ejb-name>ItemService</ejb-name>
			<ejb-class>fr.fluxx.core.service.ItemService</ejb-class>
			<session-type>Stateless</session-type>
		</session>
		<session>
			<ejb-name>OpmlService</ejb-name>
			<ejb-class>fr.fluxx.core.service.OpmlService</ejb-class>
			<session-type>Stateless</session-type>
		</session>
		<session>
			<ejb-name>RssService</ejb-name>
			<ejb-class>fr.fluxx.core.service.RssService</ejb-class>
			<session-type>Stateful</session-type>
		</session>
		<session>
			<ejb-name>ScheduledUpdateService</ejb-name>
			<ejb-class>fr.fluxx.core.service.ScheduledUpdateService</ejb-class>
			<session-type>Singleton</session-type>
		</session>
	</enterprise-beans>
</ejb-jar>
